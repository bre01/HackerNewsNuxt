#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules/nuxt/bin/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules/nuxt/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules/nuxt/bin/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules/nuxt/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/nuxt@3.8.0_vite@4.5.0/node_modules:/Users/bre/HackerNewsNuxt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
